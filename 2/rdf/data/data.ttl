# vocabularies _________________________________________________________________

# 3rd party

@prefix dbpedia-owl: <https://dbpedia.org/ontology/> .
@prefix gtfs: <http://vocab.gtfs.org/terms#> .
@prefix pext: <http://www.ontotext.com/proton/protonext#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <https://www.w3.org/TR/xmlschema-2/#> . # not standard

# @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# custom

@prefix ex: <http://example.org/vocabulary/> .

# classes ______________________________________________________________________

# Vehicle ~ pext:Vehicle

# Train ~ dbpedia-owl:Train
dbpedia-owl:Train rdfs:subClassOf pext:Vehicle .

ex:Bus a rdfs:Class ;
    rdfs:label "bus"@en ;
    rdfs:comment "Public transport road vehicle"@en .
ex:Bus rdfs:subClassOf pext:Vehicle .

# Transit agency ~ gtfs:Agency

ex:Producer a rdfs:Class ;
    rdfs:label "producer"@en ;
    rdfs:comment "A company-producer of different kinds of vehicles"@en .

# Transit route ~ gtfs:Route

ex:Connection a rdfs:Class ;
    rdfs:label "connection"@en ;
    rdfs:comment "A particular instance of a transit route which has its id. Each connection goes through the set of stops"@en .

# Stop ~ gtfs:Stop

# Station ~ dbpedia-owl:Station

ex:TrainStation a rdfs:Class ;
    rdfs:label "train station"@en ;
    rdfs:comment "Named train station."@en .
ex:TrainStation rdfs:subClassOf dbpedia-owl:Station .

ex:BusStation a rdfs:Class ;
    rdfs:label "bus station"@en ;
    rdfs:comment "Named bus station."@en.
ex:BusStation rdfs:subClassOf dbpedia-owl:Station .

# properties ___________________________________________________________________

ex:legalName a rdf:Property ;
    rdfs:label "legal name"@en ;
    rdfs:comment "Company legal name"@en ;
    rdfs:domain rdfs:Class ;
    rdfs:range xsd:string .

ex:capacity a rdf:Property ;
    rdfs:label "capacity"@en ;
    rdfs:comment "Number of people who can fit to a structure"@en ;
    rdfs:domain rdfs:Class ;
    rdfs:range xsd:integer .

ex:electricMotor a rdf:Property ;
    rdfs:label "Is motor electric?"@en ;
    rdfs:comment "Indication if a vehicle has electric motor or not"@en ;
    rdfs:domain dbpedia-owl:Train ;
    rdfs:range xsd:boolean .

ex:europeanVehicleNumber a rdf:Property ;
    rdfs:label "european vehicle number"@en ;
    rdfs:comment "Vehicle number issued by a European regulator."@en ;
    rdfs:domain dbpedia-owl:Train ;
    rdfs:range xsd:string .

ex:licensePlateNumber a rdf:Property ;
    rdfs:label "license plate number"@en ;
    rdfs:comment "License plate number issued by a regulator."@en ;
    rdfs:domain ex:Bus ;
    rdfs:range xsd:string .

ex:phone a rdf:Property ;
    rdfs:label "phone number"@en ;
    rdfs:comment "Mobile or stationary phone number"@en ;
    rdfs:domain gtfs:Agency ;
    rdfs:range xsd:string .

ex:email a rdf:Property ;
    rdfs:label "email"@en ;
    rdfs:comment "E-mail address"@en ;
    rdfs:domain gtfs:Agency ;
    rdfs:range xsd:string .

ex:consumption a rdf:Property ;
    rdfs:label "consumption"@en ;
    rdfs:comment "Vehicle consumption of gasoline or electricity per km."@en ;
    rdfs:domain pext:Vehicle ;
    rdfs:range xsd:integer .

ex:purchasePrice a rdf:Property ;
    rdfs:label "purchase price"@en ;
    rdfs:comment "Vehicle purchase price in CZK."@en ;
    rdfs:domain pext:Vehicle ;
    rdfs:range xsd:integer .

ex:length a rdfs:Property ;
	rdfs:label "length"@en ;
	rdfs:comment "A length of route in kilometers."@en ;
	rdfs:domain gtfs:Route ;
	rdfs:range xsd:decimal .

ex:validFrom a rdfs:Property ;
	rdfs:label "valid from"@en ;
	rdfs:comment "A date since when was operation on a route allowed."@en ;
	rdfs:domain gtfs:Route ;
	rdfs:range xsd:date .

ex:validTo a rdfs:Property ;
	rdfs:label "valid to"@en ;
	rdfs:comment "Last date of allowed operation on a route."@en ;
	rdfs:domain gtfs:Route ;
	rdfs:range xsd:date .

ex:gps a rdfs:Property ;
	rdfs:label "gps"@en ;
	rdfs:comment "Exact point on a map where stop is located."@en ;
	rdfs:domain gtfs:Stop ;
	rdfs:range xsd:string .

ex:arrivalTime a rdfs:Property ;
	rdfs:label "arrival time"@en ;
	rdfs:comment "Arrival time of a vehicle."@en ;
	rdfs:domain gtfs:Stop ;
	rdfs:range xsd:time .

ex:departureTime a rdfs:Property ;
	rdfs:label "departure time"@en ;
	rdfs:comment "Departure time of a vehicle."@en ;
	rdfs:domain gtfs:Stop ;
	rdfs:range xsd:time .

ex:stationName a rdfs:Property ;
    rdfs:label "station name"@en ;
	rdfs:comment "Actual station name."@en ;
	rdfs:domain dbpedia-owl:Station ;
	rdfs:range xsd:string .

# associations _________________________________________________________________

ex:owns a rdf:Property ;
    rdfs:label "owns"@en ;
    rdfs:comment "A transit agency owns a vehicle."@en ;
    rdfs:domain gtfs:Agency ;
    rdfs:range pext:Vehicle .

ex:produces a rdf:Property ;
    rdfs:label "produces"@en ;
    rdfs:comment "A producer produces a vehicle."@en ;
    rdfs:domain ex:Producer ;
    rdfs:range pext:Vehicle .

ex:rides a rdf:Property ;
    rdfs:label "rides"@en ;
    rdfs:comment "A vehicle rides a connection."@en ;
    rdfs:domain pext:Vehicle ;
    rdfs:range ex:Connection .

ex:belongsTo a rdf:Property ;
    rdfs:label "belongs to"@en ;
    rdfs:comment "A connection belongs to a transit route."@en ;
    rdfs:domain ex:Connection ;
    rdfs:range gtfs:Route .

ex:connects a rdf:Property ;
    rdfs:label "connects"@en ;
    rdfs:comment "A connection connects a set of stops."@en ;
    rdfs:domain ex:Connection ;
    rdfs:range gtfs:Stop .

ex:followedBy a rdf:Property ;
    rdfs:label "followed by"@en ;
    rdfs:comment "A stop is followed by the next stop."@en ;
    rdfs:domain gtfs:Stop ;
    rdfs:range gtfs:Stop .

ex:serves a rdf:Property ;
    rdfs:label "serves"@en ;
    rdfs:comment "A station serves a stop."@en ;
    rdfs:domain dbpedia-owl:Station ;
    rdfs:range gtfs:Stop .

ex:startsWith a rdf:Property ;
	rdfs:label "starts with"@end ;
	rdfs:comment "Name of a station, where the route begins."@en ;
	rdfs:domain gtfs:Route ;
	rdfs:range dbpedia-owl:Station .

ex:endsWith a rdf:Property ;
	rdfs:label "ends with"@end ;
	rdfs:comment "Name of a stop, where the route ends."@en ;
	rdfs:domain gtfs:Route ;
	rdfs:range dbpedia-owl:Station .

