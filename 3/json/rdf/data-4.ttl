@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/vocabulary/Bus> a rdfs:Class ;
    rdfs:label "bus"@en ;
    rdfs:comment "Public transport road vehicle"@en ;
    rdfs:subClassOf <http://www.ontotext.com/proton/protonext#Vehicle> .

<http://example.org/vocabulary/BusStation> a rdfs:Class ;
    rdfs:label "bus station"@en ;
    rdfs:comment "Named bus station."@en ;
    rdfs:subClassOf <https://dbpedia.org/ontology/Station> .

<http://example.org/vocabulary/Connection> a rdfs:Class ;
    rdfs:label "connection"@en ;
    rdfs:comment "A particular instance of a transit route which has its id. Each connection goes through the set of stops"@en .

<http://example.org/vocabulary/Producer> a rdfs:Class ;
    rdfs:label "producer"@en ;
    rdfs:comment "A company-producer of different kinds of vehicles"@en .

<http://example.org/vocabulary/TrainStation> a rdfs:Class ;
    rdfs:label "train station"@en ;
    rdfs:comment "Named train station."@en ;
    rdfs:subClassOf <https://dbpedia.org/ontology/Station> .

<http://example.org/vocabulary/arrivalTime> a rdf:Property ;
    rdfs:label "arrival time"@en ;
    rdfs:comment "Arrival time of a vehicle."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Stop> ;
    rdfs:range xsd:time .

<http://example.org/vocabulary/belongsTo> a rdf:Property ;
    rdfs:label "belongs to"@en ;
    rdfs:comment "A connection belongs to a transit route."@en ;
    rdfs:domain <http://example.org/vocabulary/Connection> ;
    rdfs:range <http://vocab.gtfs.org/terms#Route> .

<http://example.org/vocabulary/capacity> a rdf:Property ;
    rdfs:label "capacity"@en ;
    rdfs:comment "Number of people who can fit to a structure"@en ;
    rdfs:domain rdfs:Class ;
    rdfs:range xsd:integer .

<http://example.org/vocabulary/connects> a rdf:Property ;
    rdfs:label "connects"@en ;
    rdfs:comment "A connection connects a set of stops."@en ;
    rdfs:domain <http://example.org/vocabulary/Connection> ;
    rdfs:range <http://vocab.gtfs.org/terms#Stop> .

<http://example.org/vocabulary/consumption> a rdf:Property ;
    rdfs:label "consumption"@en ;
    rdfs:comment "Vehicle consumption of gasoline or electricity per km."@en ;
    rdfs:domain <http://www.ontotext.com/proton/protonext#Vehicle> ;
    rdfs:range xsd:integer .

<http://example.org/vocabulary/departureTime> a rdf:Property ;
    rdfs:label "departure time"@en ;
    rdfs:comment "Departure time of a vehicle."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Stop> ;
    rdfs:range xsd:time .

<http://example.org/vocabulary/electricMotor> a rdf:Property ;
    rdfs:label "Is motor electric?"@en ;
    rdfs:comment "Indication if a vehicle has electric motor or not"@en ;
    rdfs:domain <https://dbpedia.org/ontology/Train> ;
    rdfs:range xsd:boolean .

<http://example.org/vocabulary/endsWith> a rdf:Property ;
    rdfs:label "ends with"@en ;
    rdfs:comment "Name of a station, where the route ends."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Route> ;
    rdfs:range <https://dbpedia.org/ontology/Station> .

<http://example.org/vocabulary/europeanVehicleNumber> a rdf:Property ;
    rdfs:label "european vehicle number"@en ;
    rdfs:comment "Vehicle number issued by a European regulator."@en ;
    rdfs:domain <https://dbpedia.org/ontology/Train> ;
    rdfs:range xsd:string .

<http://example.org/vocabulary/followedBy> a rdf:Property ;
    rdfs:label "followed by"@en ;
    rdfs:comment "A stop is followed by the next stop."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Stop> ;
    rdfs:range <http://vocab.gtfs.org/terms#Stop> .

<http://example.org/vocabulary/gps> a rdf:Property ;
    rdfs:label "gps"@en ;
    rdfs:comment "Exact point on a map where stop is located."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Stop> ;
    rdfs:range xsd:string .

<http://example.org/vocabulary/legalName> a rdf:Property ;
    rdfs:label "legal name"@en ;
    rdfs:comment "Company legal name"@en ;
    rdfs:domain rdfs:Class ;
    rdfs:range xsd:string .

<http://example.org/vocabulary/length> a rdf:Property ;
    rdfs:label "length"@en ;
    rdfs:comment "A length of route in kilometers."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Route> ;
    rdfs:range xsd:decimal .

<http://example.org/vocabulary/licensePlateNumber> a rdf:Property ;
    rdfs:label "license plate number"@en ;
    rdfs:comment "License plate number issued by a regulator."@en ;
    rdfs:domain <http://example.org/vocabulary/Bus> ;
    rdfs:range xsd:string .

<http://example.org/vocabulary/owns> a rdf:Property ;
    rdfs:label "owns"@en ;
    rdfs:comment "A transit agency owns a vehicle."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Agency> ;
    rdfs:range <http://www.ontotext.com/proton/protonext#Vehicle> .

<http://example.org/vocabulary/produces> a rdf:Property ;
    rdfs:label "produces"@en ;
    rdfs:comment "A producer produces a vehicle."@en ;
    rdfs:domain <http://example.org/vocabulary/Producer> ;
    rdfs:range <http://www.ontotext.com/proton/protonext#Vehicle> .

<http://example.org/vocabulary/purchasePrice> a rdf:Property ;
    rdfs:label "purchase price"@en ;
    rdfs:comment "Vehicle purchase price in CZK."@en ;
    rdfs:domain <http://www.ontotext.com/proton/protonext#Vehicle> ;
    rdfs:range xsd:integer .

<http://example.org/vocabulary/rides> a rdf:Property ;
    rdfs:label "rides"@en ;
    rdfs:comment "A vehicle rides a connection."@en ;
    rdfs:domain <http://www.ontotext.com/proton/protonext#Vehicle> ;
    rdfs:range <http://example.org/vocabulary/Connection> .

<http://example.org/vocabulary/serves> a rdf:Property ;
    rdfs:label "serves"@en ;
    rdfs:comment "A station serves a stop."@en ;
    rdfs:domain <https://dbpedia.org/ontology/Station> ;
    rdfs:range <http://vocab.gtfs.org/terms#Stop> .

<http://example.org/vocabulary/startsWith> a rdf:Property ;
    rdfs:label "starts with"@en ;
    rdfs:comment "Name of a station, where the route begins."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Route> ;
    rdfs:range <https://dbpedia.org/ontology/Station> .

<http://example.org/vocabulary/stationName> a rdf:Property ;
    rdfs:label "station name"@en ;
    rdfs:comment "Actual station name."@en ;
    rdfs:domain <https://dbpedia.org/ontology/Station> ;
    rdfs:range xsd:string .

<http://example.org/vocabulary/validFrom> a rdf:Property ;
    rdfs:label "valid from"@en ;
    rdfs:comment "A date since when was operation on a route allowed."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Route> ;
    rdfs:range xsd:date .

<http://example.org/vocabulary/validTo> a rdf:Property ;
    rdfs:label "valid to"@en ;
    rdfs:comment "Last date of allowed operation on a route."@en ;
    rdfs:domain <http://vocab.gtfs.org/terms#Route> ;
    rdfs:range xsd:date .

<https://dbpedia.org/ontology/Train> rdfs:subClassOf <http://www.ontotext.com/proton/protonext#Vehicle> .
