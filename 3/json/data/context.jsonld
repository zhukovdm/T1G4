{
  "@context": {
    "addr": "http://www.w3.org/Addressing/schemes.html#",
    "dbpedia-owl": "https://dbpedia.org/ontology/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "gtfs": "http://vocab.gtfs.org/terms#",
    "pext": "http://www.ontotext.com/proton/protonext#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "ex": "http://example.org/vocabulary/",

    "agency": "https://T1G4.DOX.cz/TransitAgency/#",
    "bus": "https://T1G4.DOX.cz/Bus/#",
    "train": "https://T1G4.DOX.cz/Train/#",
    "producer": "https://T1G4.DOX.cz/Producer/#",
    "route": "https://T1G4.DOX.cz/TransitRoute/#",
    "conn": "https://T1G4.DOX.cz/Connection/#",
    "stop": "https://T1G4.DOX.cz/Stop/#",
    "station": "https://T1G4.DOX.cz/Station/#",

    "Agency": "gtfs:Agency",
    "Bus": "ex:Bus",
    "Train": "dbpedia-owl:Train",
    "Producer": "ex:Producer",
    "Route": "gtfs:Route",
    "Connection": "ex:Connection",
    "Stop": "gtfs:Stop",
    "TrainStation": "ex:TrainStation",
    "BusStation": "ex:BusStation",

    "id": "@id",
    "type": "@type",
    "legalName": {
      "@id": "ex:legalName",
      "@container": "@language"
    },
    "capacity": {
      "@id": "ex:capacity",
      "@type": "xsd:integer"
    },
    "electricMotor": {
      "@id": "ex:electricMotor",
      "@type": "xsd:boolean"
    },
    "europeanVehicleNumber": {
      "@id": "ex:europeanVehicleNumber",
      "@type": "xsd:string"
    },
    "licensePlateNumber": {
      "@id": "ex:licensePlateNumber",
      "@type": "xsd:string"
    },
    "consumption": {
      "@id": "ex:consumption",
      "@type": "xsd:integer"
    },
    "purchasePrice": {
      "@id": "ex:purchasePrice",
      "@type": "xsd:integer"
    },
    "length": {
      "@id": "ex:length",
      "@type": "xsd:decimal"
    },
    "validFrom": {
      "@id": "ex:validFrom",
      "@type": "xsd:date"
    },
    "validTo": {
      "@id": "ex:validTo",
      "@type": "xsd:date"
    },
    "gps": {
      "@id": "ex:gps",
      "@type": "xsd:string"
    },
    "arrivalTime": {
      "@id": "ex:arrivalTime",
      "@type": "xsd:time"
    },
    "departureTime": {
      "@id": "ex:departureTime",
      "@type": "xsd:time"
    },
    "stationName": {
      "@id": "ex:stationName",
      "@container": "@language"
    },
    "phone": {
      "@id": "foaf:phone",
      "@type": "@id"
    },
    "email": {
      "@id": "foaf:mbox",
      "@type": "@id"
    },
    "owns": {
      "@id": "ex:owns"
    },
    "produces": {
      "@id": "ex:produces"
    },
    "rides": {
      "@id": "ex:rides"
    },
    "belongsTo": {
      "@id": "ex:belongsTo"
    },
    "connects": {
      "@id": "ex:connects"
    },
    "followedBy": {
      "@id": "ex:followedBy"
    },
    "serves": {
      "@id": "ex:serves"
    },
    "startsWith": {
      "@id": "ex:startsWith"
    },
    "endsWith": {
      "@id": "ex:endsWith"
    }
  }
}
